###############################
# Sample data for testing
###############################

@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

###############################
# Organisations
###############################

<https://museum.example.org/>
  a foaf:Organization ;
  foaf:name "Museum"@nl, "Museum"@en .

<https://archive.example.org/>
  a foaf:Organization ;
  foaf:name "Archive" . # No language indicator

<https://library.example.org/>
  a foaf:Organization ;
  foaf:name "Library"@en . # Only English

###############################
# Licenses
###############################

<http://creativecommons.org/publicdomain/zero/1.0/>
  a foaf:Document ;
  foaf:name "Publiek domein"@nl, "Public Domain"@en .

# https version of the above
<https://creativecommons.org/publicdomain/zero/1.0/>
  a foaf:Document ;
  foaf:name "Publiek domein"@nl, "Public Domain"@en .

# Specific Dutch version (bad URI)
<http://creativecommons.org/publicdomain/zero/1.0/deed.nl>
  a foaf:Document ;
  foaf:name "Publiek domein"@nl . # Only Dutch

