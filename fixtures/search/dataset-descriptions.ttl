###############################
# Sample data for testing
###############################

@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

###############################
# Organisations
###############################

<https://museum.example.org/>
  a foaf:Organization ;
  foaf:name "Museum"@nl . # Only in Dutch

<https://archive.example.org/>
  a foaf:Organization ;
  foaf:name "Archive"@en . # Only English

<https://library.example.org/>
  a foaf:Organization ;
  foaf:name "Library" . # No language indicator

<https://research.example.org/>
  a foaf:Organization ;
  foaf:name "Research Organisation" . # No language indicator

###############################
# Licenses
###############################

<https://creativecommons.org/publicdomain/zero/1.0/>
  a foaf:Document ;
  foaf:name "Creative Commons Public Domain (CC0 1.0)" . # No language indicator

<https://creativecommons.org/licenses/by/4.0/>
  a foaf:Document ;
  foaf:name "Creative Commons Attribution (CC BY 4.0)" . # No language indicator

<https://opendatacommons.org/licenses/by/1.0/>
  a foaf:Document ;
  foaf:name "Open Data Commons Attribution (ODC-By 1.0)" . # No language indicator

