{
  "name": "@colonial-collections/enricher",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "lint": "gts check",
    "test": "jest --passWithNoTests --testPathIgnorePatterns '<rootDir>/src/.*/*.integration.test.ts'",
    "test:ci": "npm test -- --ci",
    "test:watch": "npm test -- --watchAll",
    "test:integration": "jest --testMatch '**/src/**/*.integration.test.ts'",
    "test:integration:watch": "npm run test:integration -- --watchAll"
  },
  "dependencies": {
    "@colonial-collections/iris": "*",
    "defu": "6.1.4",
    "fetch-sparql-endpoint": "4.1.0",
    "rdf-data-factory": "1.1.2",
    "rdf-object": "1.14.0",
    "rdf-serialize": "2.2.2",
    "rdf-stores": "1.0.0",
    "stream-to-string": "1.2.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@rdfjs/types": "1.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.10.6",
    "eslint-config-custom": "*",
    "gts": "5.2.0",
    "jest": "29.7.0",
    "ts-jest": "29.1.2",
    "tsconfig": "*",
    "typescript": "5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testTimeout": 60000,
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ]
  }
}
